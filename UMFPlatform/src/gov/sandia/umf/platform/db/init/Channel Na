N2A.schema=1
name=Channel Na
parent=$inherit("Channel")
I = G * m ^ 3 * h * (E - V)
m' = alpha_m * (1 - m) - beta_m * m
h' = alpha_h * (1 - h) - beta_h * h
alpha_m =: (25 - V) / (10 * (exp((25 - V) / 10) - 1))
beta_m =: 4 * exp(-V / 18)
alpha_h =: 0.07 * exp(-V / 20)
beta_h =: 1 / (exp((30 - V) / 10) + 1)
G = 120
E = 115
m = 0.05 @ $init
h = 0.31 @ $init

