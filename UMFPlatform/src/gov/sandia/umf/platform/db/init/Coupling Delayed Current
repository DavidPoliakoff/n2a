N2A.schema=1
$metadata
 backend.stpu.component=inp2lif
A=$connect("Input Current")
ABweight=1.0
 $metadata
  backend.stpu.param=SynStr
Adelay=5.0ms
 $metadata
  backend.stpu.param=TmpOff
B=$connect("Neuron LIF")
B.I=A.I*ABweight@$event(clocked,Adelay,"change")
clocked
 @$init=0
 @clocked==0=1
 @clocked==1=0
