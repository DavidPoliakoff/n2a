N2A.schema=1
$inherit="Channel"
E=-12mV
 $metadata
  backend.xyce.param="eK"
G=36mS
 $metadata
  backend.xyce.param="gK"
I=G*n^4*(E-V)
alpha_n=:(10-mV)/(100*(exp((10-mV)/10)-1))
 $metadata
  backend.xyce.ignore
beta_n=:0.125*exp(-mV/80)
 $metadata
  backend.xyce.ignore
mV=:V/1mV
n=0.6@$init
n'=(alpha_n*(1-n)-beta_n*n)/1ms
 $metadata
  backend.xyce.internalVar="n"
