N2A.schema=1
$metadata
 name=Channel K
$inherit="Channel"
I = G * n ^ 4 * (E - V)
n' = alpha_n * (1 - n) - beta_n * n
 $metadata
  xyce.internalVar="n"
alpha_n =: (10 - V) / (100 * (exp((10 - V) / 10) - 1))
 $metadata
  xyce.ignore
beta_n =: 0.125 * exp(-V / 80)
 $metadata
  xyce.ignore
G = 36
 $metadata
  xyce.param="gK"
E = -12
 $metadata
  xyce.param="eK"
n = 0.6 @ $init

